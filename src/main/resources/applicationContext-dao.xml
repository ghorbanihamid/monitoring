<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:util="http://www.springframework.org/schema/util"
       xmlns:tx="http://www.springframework.org/schema/tx"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:aop="http://www.springframework.org/schema/aop"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                           http://www.springframework.org/schema/tx
                           http://www.springframework.org/schema/tx/spring-tx-3.0.xsd"
       default-autowire="byName">


    <bean id="parametersDao"     class="com.soshiant.server.dao.parameters.ParametersDaoHibernateImpl" />

    <bean id="smsDao"            class="com.soshiant.server.dao.parameters.SmsDaoHibernateImpl" />

    <bean id="appLogDao"         class="com.soshiant.server.dao.log.AppLogDaoHibernateImpl" />

    <bean id="userDao"           class="com.soshiant.server.dao.user.UserDaoHibernateImpl" />

    <bean id="groupDao"          class="com.soshiant.server.dao.group.GroupDaoHibernateImpl" />

    <bean id="staffDao"          class="com.soshiant.server.dao.staff.StaffDaoHibernateImpl" />

    <bean id="positionDao"       class="com.soshiant.server.dao.position.PositionDaoHibernateImpl" />

    <bean id="staffPositionDao"  class="com.soshiant.server.dao.staffPosition.StaffPositionDaoHibernateImpl" />

    <bean id="serverDao"         class="com.soshiant.server.dao.server.ServerDaoHibernateImpl" />

    <bean id="monitoringDao"     class="com.soshiant.server.dao.monitoring.MonitoringDaoHibernateImpl" />


</beans>